md-content(flex, layout-padding)
  md-input-container
    label Api-URL
    input(type="text" ng-model="home.url")
  md-button(ng-click="home.install()") install pack
  md-input-container
    label Email
    input(type="email", ng-model="home.user.email")
  md-input-container
    label Password
    input(type="password", ng-model="home.user.password")
  md-button(ng-click="home.login()") login
  pre {{home.data | json}}
